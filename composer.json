{
  "name": "osclass/platform",
  "description": "With Osclass, get your own  classifieds site for free. Build your own Osclass installation and start advertising real estate, jobs or whatever you want- in minutes! ",
  "keywords": ["osclass","classified","open-source","php-classified","cms"],
  "homepage": "https://github.com/claxifieds/platform",
  "license": "Apache-2.0",
  "type": "project",
  "autoload": {
    "psr-0": {
      "": "src/"
    },
    "exclude-from-classmap": [
      "**/Test/**"
    ]
  },
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:claxifieds/core.git"
    },
    {
      "type": "git",
      "url": "git@github.com:claxifieds/akismet.git"
    },
    {
      "type": "git",
      "url": "git@github.com:claxifieds/ajaxuploader.git"
    }
  ],
  "require": {
    "php": ">=5.6.20",
    "ext-json": "*",
    "ext-openssl": "*",
    "ext-mysqli": "*",
    "ext-zip": "*",
    "ext-curl": "*",
    "ext-posix": "*",
    "ext-fileinfo": "*",
    "ext-gd": "*",
    "ezyang/htmlpurifier": "4.11.x",
    "phpmailer/phpmailer": "5.2.x",
    "pclzip/pclzip": "2.8.2",
    "phpseclib/phpseclib": "2.0.x",
    "pensiero/php-openssl-cryptor": "0.1.0",
    "google/recaptcha": "1.1.x",
    "gettext/gettext": "4.6.x",
    "servicescout/akismet": "0.5.2",
    "claxifieds/ajaxuploader": "0.1.0",
    "claxifieds/core": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "~4.2",
    "squizlabs/php_codesniffer": "3.*",
    "phpmd/phpmd": "@stable",
    "pdepend/pdepend": "2.4.0",
    "friendsofphp/php-cs-fixer": "^2.16"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
